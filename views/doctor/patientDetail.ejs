<!doctype html>
<html lang="en">

<head>
    <title>Patient | Digital Health Record Portal</title>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no, user-scalable=no">
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-BmbxuPwQa2lc/FVzBcNJ7UAyJxM6wuqIj61tLrc4wSX0szH/Ev+nYRRuWlolflfl" crossorigin="anonymous">
    <!-- Bootstrap Icons -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.0/font/bootstrap-icons.css">


    <!-- Font -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins&display=swap" rel="stylesheet">
    <!-- Custom Styling -->
    <link rel="stylesheet" href="/css/doctor/main.css">
    <link rel="stylesheet" href="/css/doctor/dashboard.css">
    <link rel="stylesheet" href="/css/doctor/patientsDetail.css">
    <!-- FontAwesome -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body>
    <!-- toolbar -->
    <div class="toolbar">
        <!-- hamburger menu -->
        <div class="toolbar-hamburger">
            <nav role="navigation">
                <div id="hamburger">
                    <input type="checkbox" />
                    <span></span>
                    <span></span>
                    <span></span>

                    <ul id="menu">
                        <div id="nav-brand">
                            <p class="fs-28 text-center">Digital Health<br>Record Portal</p>
                            <div class="links fs-16">
                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="1" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-share">
                                    <path d="M4 12v8a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2v-8">
                                    </path>
                                    <polyline points="16 6 12 2 8 6">
                                    </polyline>
                                    <line x1="12" y1="2" x2="12" y2="15">
                                    </line>
                                </svg>
                                <a href="/doctor/dashboard/logout">Logout</a>
                                <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="1" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-info">
                                    <circle cx="12" cy="12" r="10">
                                    </circle>
                                    <line x1="12" y1="16" x2="12" y2="12">
                                    </line>
                                    <line x1="12" y1="8" x2="12.01" y2="8">
                                    </line>
                                </svg>

                                <a href="/about" target="_blank">About</a>
                            </div>
                        </div>
                        <div class="menu-items mt-3">
                            <a href="/doctor/dashboard/" class="menu-item-single">
                                <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-home">
                                    <path d="M3 9l9-7 9 7v11a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z">
                                    </path>
                                    <polyline points="9 22 9 12 15 12 15 22">
                                    </polyline>
                                </svg>
                                <li>Home</li>
                            </a>
                            <a href="/doctor/dashboard/current" class="menu-item-single">
                                <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-edit">
                                    <path d="M11 4H4a2 2 0 0 0-2 2v14a2 2 0 0 0 2 2h14a2 2 0 0 0 2-2v-7">
                                    </path>
                                    <path d="M18.5 2.5a2.121 2.121 0 0 1 3 3L12 15l-4 1 1-4 9.5-9.5z">
                                    </path>
                                </svg>

                                <li>Current Patients</li>
                            </a>
                            <a href="/doctor/dashboard/previous" class="menu-item-single">
                                <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-archive">
                                    <polyline points="21 8 21 21 3 21 3 8">
                                    </polyline>
                                    <rect x="1" y="3" width="22" height="5">
                                    </rect>
                                    <line x1="10" y1="12" x2="14" y2="12">
                                    </line>
                                </svg>

                                <li>Previous Patients</li>
                            </a>
                            <a href="/doctor/dashboard/chat" class="menu-item-single">
                                <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-message-square">
                                    <path d="M21 15a2 2 0 0 1-2 2H7l-4 4V5a2 2 0 0 1 2-2h14a2 2 0 0 1 2 2z">
                                    </path>
                                </svg>
                                <li>Chat</li>
                            </a>
                            <a href="/doctor/dashboard/settings" class="menu-item-single">
                                <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-settings">
                                    <circle cx="12" cy="12" r="3">
                                    </circle>
                                    <path
                                        d="M19.4 15a1.65 1.65 0 0 0 .33 1.82l.06.06a2 2 0 0 1 0 2.83 2 2 0 0 1-2.83 0l-.06-.06a1.65 1.65 0 0 0-1.82-.33 1.65 1.65 0 0 0-1 1.51V21a2 2 0 0 1-2 2 2 2 0 0 1-2-2v-.09A1.65 1.65 0 0 0 9 19.4a1.65 1.65 0 0 0-1.82.33l-.06.06a2 2 0 0 1-2.83 0 2 2 0 0 1 0-2.83l.06-.06a1.65 1.65 0 0 0 .33-1.82 1.65 1.65 0 0 0-1.51-1H3a2 2 0 0 1-2-2 2 2 0 0 1 2-2h.09A1.65 1.65 0 0 0 4.6 9a1.65 1.65 0 0 0-.33-1.82l-.06-.06a2 2 0 0 1 0-2.83 2 2 0 0 1 2.83 0l.06.06a1.65 1.65 0 0 0 1.82.33H9a1.65 1.65 0 0 0 1-1.51V3a2 2 0 0 1 2-2 2 2 0 0 1 2 2v.09a1.65 1.65 0 0 0 1 1.51 1.65 1.65 0 0 0 1.82-.33l.06-.06a2 2 0 0 1 2.83 0 2 2 0 0 1 0 2.83l-.06.06a1.65 1.65 0 0 0-.33 1.82V9a1.65 1.65 0 0 0 1.51 1H21a2 2 0 0 1 2 2 2 2 0 0 1-2 2h-.09a1.65 1.65 0 0 0-1.51 1z">
                                    </path>
                                </svg>
                                <li>Settings</li>
                            </a>
                        </div>
                    </ul>
                </div>
            </nav>
        </div>
        <!-- Heading -->
        <div class="toolbar-header">
            <p class="fs-32">Patients Detail</p>
        </div>
        <!-- Profile -->
        <div class="toolbar-profile">
            <div class="profile-name">
                <p>
                    <%= fullName %>
                </p>
            </div>
            <div class="profile-image">
                <!-- doctor-svg -->
                <?xml version="1.0"?>
                <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                    xmlns:svgjs="http://svgjs.com/svgjs" version="1.1" width="32" height="32" x="0" y="0"
                    viewBox="0 0 480 480" style="enable-background:new 0 0 512 512" xml:space="preserve" class="">
                    <g>
                        <path xmlns="http://www.w3.org/2000/svg"
                            d="m372 87.54c0 101.629.015 95.29-.032 96.356-.255 5.786-8.968 5.496-8.968-.296v-38.37c0-39.697-32.301-72-72-72h-102c-39.692 0-72 32.296-72 72v38.37c0 5.791-8.714 6.081-8.968.296-.047-1.066-.032 5.466-.032-98.896 0-17.45 12.84-31.96 29.57-34.58 14.36-30.79 44.96-50.42 79.28-50.42h67.61c48.347 0 87.54 39.193 87.54 87.54zm-81 1.69h-102c-30.88 0-56 25.13-56 56 0 .235-.006 66.346.01 66.58.91 58.23 48.56 105.31 106.99 105.31 58.72 0 106.54-47.54 107-106.16v-65.73c0-30.88-25.12-56-56-56zm34 257.67v29.691c0 3.437 2.193 6.499 5.456 7.577 17.139 5.661 29.544 21.821 29.544 40.832v36.726c0 4.261-3.215 7.982-7.468 8.257-4.659.301-8.532-3.389-8.532-7.983v-36.425c0-14.692-11.497-27.127-26.183-27.563-15.259-.453-27.817 11.829-27.817 26.988v36.726c0 4.261-3.215 7.982-7.468 8.257-4.659.301-8.532-3.389-8.532-7.983v-37c0-19.011 12.405-35.171 29.544-40.832 3.264-1.078 5.456-4.14 5.456-7.577v-34.453c0-1.911-2.223-2.978-3.72-1.837-65.432 49.879-61.542 47.722-65.275 47.699-3.895-.023.574 2.5-65.293-47.704-1.443-1.1-3.712-.143-3.712 1.836v45.42c0 3.088 1.769 5.921 4.571 7.219 10.723 4.968 18.04 16.082 17.389 28.797-.771 15.053-12.91 27.38-27.95 28.365-17.448 1.144-32.01-12.726-32.01-29.933 0-12.029 7.118-22.42 17.362-27.197 2.823-1.316 4.638-4.135 4.638-7.25v-40.783c0-2.38-2.069-4.263-4.431-3.979-40.905 4.933-72.569 39.708-72.569 81.949v47.26c0 4.418 3.582 8 8 8h308c4.418 0 8-3.582 8-8v-47.628c0-42.089-31.781-76.076-72.526-81.445-2.376-.313-4.474 1.576-4.474 3.973zm-80.15 19.345 42.679-32.531c4.321-3.294.674-10.215-4.439-8.376-30.055 10.809-59.778 9.896-86.037.049-5.305-1.989-8.898 5.038-4.59 8.322l42.688 32.537c2.864 2.182 6.834 2.182 9.699-.001zm-95.85 55.755c0 7.72 6.28 14 14 14s14-6.28 14-14-6.28-14-14-14-14 6.28-14 14z"
                            fill="#ffffff" data-original="#000000" class="" />
                    </g>
                </svg>
            </div>
        </div>
    </div>
    <!-- toolbar space fix -->
    <div class="toolbar-space"></div>
    <!-- alert for home only -->
    <main>
        <% if (status) { %>
        <div class='status-wrap'>
            <p class="status" id="mess">
                <%= status %>
            </p>
        </div>
        <% } %>
        <!-- For Basic Details -->
        <button type="button" class="collapsible fs-28">Basic Details</button>
        <div class="content-wrapper">
            <div class="row" style="margin-top: 20px;">
                <div class="col-sm-6 row patient-body">
                    <div class="col-5">
                        <p class="bold">Name</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.profile.firstName %>&nbsp;<%= patient.profile.lastName %>
                        </p>
                    </div>
                    <div class="col-5">
                        <p class="bold">DOB</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.profile.DOB.toLocaleDateString("en-US") %>
                        </p>
                    </div>
                    <div class="col-5">
                        <p class="bold">Gender</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.profile.gender %>
                        </p>
                    </div>
                </div>
                <div class="col-sm-6 row patient-body">
                    <div class="col-5">
                        <p class="bold">Contact</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.profile.mobileNo %>
                        </p>
                    </div>
                    <div class="col-5">
                        <p class="bold">Allergies</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.medical.allergies.join(',') %>
                        </p>
                    </div>
                    <div class=" col-5">
                        <p class="bold">Blood Group</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.profile.bloodGroup %>
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="spaceFix"></div>
        <!-- For Medical Details -->
        <button type="button" class="collapsible fs-28">Medical Details</button>
        <div class="content-wrapper">
            <div class="row" style="margin-top: 20px;">
                <div class="col-sm-6 row patient-body">
                    <div class="col-5">
                        <p class="bold">Medical<br>Diagnosis</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.medical.medicalDiagnosis %>
                        </p>
                    </div>
                    <div class="col-5">
                        <p class="bold">Allergies</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.medical.allergies.join(',')%>
                        </p>
                    </div>
                </div>
                <div class="col-sm-6 row patient-body">
                    <div class="col-5">
                        <p class="bold">Functional<br> Status</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.medical.functionalStatus %>
                        </p>
                    </div>
                    <div class="col-5">
                        <p class="bold">Equipment <br>Device</p>
                    </div>
                    <div class="col-7">
                        <p>
                            <%= patient.medical.equipmentDevice %>
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="spaceFix"></div>
        <!-- For Current Treatment -->
        <button type="button" class="collapsible  fs-28">Current Treatments</button>
        <!-- All current treatmenst -->
        <div class="content-wrapper">
            <% if (patient.currentTreatments.length> 0 ) { %>
                <% patient.currentTreatments.forEach((currTreat)=> { %>
                    <div class="wrapper mt-2">
                        <!-- block -->
                        <div class="content">
                            <div class="patient-detail">
                                <div class="id">
                                    <p class="bold">Health ID :</p>
                                    <p>
                                        <%= currTreat.healthID %>
                                    </p>
                                </div>
                                <div class="name">
                                    <p class="bold">Treatment No :</p>
                                    <p>
                                        <%= currTreat.treatmentNo %>
                                    </p>
                                </div>

                                <div class="name">
                                    <p class="bold">Doctor :</p>
                                    <p>
                                        <%= currTreat.doctor.profile.firstName %>
                                            <%= currTreat.doctor.profile.lastName %>
                                    </p>
                                </div>
                                <div class="date">
                                    <p class="bold">Date:</p>
                                    <p>
                                        <%= currTreat.startDate.toLocaleDateString("en-GB") %>
                                    </p>
                                </div>
                            </div>
                            <div class="patient-action text-center">
                                <div class="view">
                                    <button class="btn btn-view" data-bs-toggle="modal"
                                        data-bs-target="#modal-<%= currTreat._id %>">View</button>
                                </div>
                            </div>
                        </div>
                        <!-- modal -->
                        <div class="modal fade" id="modal-<%= currTreat._id %>" tabindex="-1"
                            aria-labelledby="exampleModalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title text-center" id="exampleModalLabel">Treatment Detail
                                        </h5>
                                        <button type="button" class="btn-close" data-bs-dismiss="modal"
                                            aria-label="Close"></button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Disease & History:
                                            </div>
                                            <div class="col-sm-8 text text-justify">
                                                <%= currTreat.history %>
                                            </div>
                                        </div>
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Prescription:
                                            </div>
                                            <div class="col-sm-8 text">
                                                <%= currTreat.prescriptions %>
                                            </div>
                                        </div>
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Medicine:
                                            </div>
                                            <div class="col-sm-8 text">
                                                <% currTreat.medicines.name.forEach((medName,i)=> {%>
                                                    <p>
                                                        <%=medName%> - <%= currTreat.medicines.time[i] %>
                                                    </p>
                                                    <% }) %>
                                            </div>
                                        </div>
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Lab Reports:
                                            </div>
                                            <div class="col-sm-8 text">
                                                <% if (currTreat.labReports.attachment) { %>
                                                    <p>Test - <%= currTreat.labReports.testName %>
                                                    </p>
                                                    <p>Lab - <%= currTreat.labReports.labName %>
                                                    </p>
                                                    <p>Attachment - <a style="color:blue"
                                                            href="/doctor/dashboard/report/<%= currTreat.labReports.attachment %>">
                                                            <%= currTreat.labReports.attachment %>
                                                        </a>
                                                    </p>
                                                    <p>Remarks - <%= currTreat.labReports.remarks %>
                                                    </p>
                                                    <p>Dated - <%= currTreat.labReports.submitDate %>
                                                    </p>
                                                    <% } else { %>
                                                        <p>No Reports</p>
                                                        <% } %>
                                            </div>
                                        </div>
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Response:
                                            </div>
                                            <div class="col-sm-8 text">
                                                <%= currTreat.response %>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-dismiss"
                                            data-bs-dismiss="modal">Dismiss</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <% }) %>
                        <% } else { %>
                            <div class="no-item" style="text-align: center;">
                                <p class="bold fs-20">No Current Treatments</p>
                            </div>
                            <% } %>
        </div>
        <!-- For Previous Treatment -->
        <div class="spaceFix"></div>
        <button type="button" class="collapsible fs-28">Previous Treatments</button>
        <!-- All previous treatments -->
        <div class="content-wrapper">
            <% if (patient.previousTreatments.length> 0) { %>
                <% patient.previousTreatments.forEach((prevTreat)=> { %>
                    <div class="wrapper mt-2">
                        <!-- block -->
                        <div class="content">
                            <div class="patient-detail">
                                <div class="id">
                                    <p class="bold">Health ID :</p>
                                    <p>
                                        <%= prevTreat.healthID %>
                                    </p>
                                </div>
                                <div class="name">
                                    <p class="bold">Treatment No :</p>
                                    <p>
                                        <%= prevTreat.treatmentNo %>
                                    </p>
                                </div>

                                <div class="name">
                                    <p class="bold">Doctor :</p>
                                    <p>
                                        <%= prevTreat.doctor.profile.firstName %>
                                            <%= prevTreat.doctor.profile.lastName %>
                                    </p>
                                </div>
                                <div class="date">
                                    <p class="bold">Date:</p>
                                    <p>
                                        <%= prevTreat.startDate.toLocaleDateString("en-GB") %>
                                    </p>
                                </div>
                            </div>
                            <div class="patient-action text-center">
                                <div class="view">
                                    <button class="btn btn-view" data-bs-toggle="modal"
                                        data-bs-target="#modal-<%= prevTreat._id %>">View</button>
                                </div>
                            </div>
                        </div>
                        <!-- modal -->
                        <div class="modal fade" id="modal-<%= prevTreat._id %>" tabindex="-1"
                            aria-labelledby="exampleModalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title text-center" id="exampleModalLabel">Treatment
                                            Detail
                                        </h5>
                                        <button type="button" class="btn-close" data-bs-dismiss="modal"
                                            aria-label="Close"></button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Disease & History:
                                            </div>
                                            <div class="col-sm-8 text text-justify">
                                                <%= prevTreat.history %>
                                            </div>
                                        </div>
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Prescription:
                                            </div>
                                            <div class="col-sm-8 text">
                                                <%= prevTreat.prescriptions %>
                                            </div>
                                        </div>
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Medicine:
                                            </div>
                                            <div class="col-sm-8 text">
                                                <% prevTreat.medicines.name.forEach((medName,i)=> {%>
                                                    <p>
                                                        <%=medName%> - <%= prevTreat.medicines.time[i] %>
                                                    </p>
                                                    <% }) %>
                                            </div>
                                        </div>
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Lab Reports:
                                            </div>
                                            <div class="col-sm-8 text">
                                                <% if (prevTreat.labReports.attachment) { %>
                                                    <p>Test - <%= prevTreat.labReports.testName %>
                                                    </p>
                                                    <p>Lab - <%= prevTreat.labReports.labName %>
                                                    </p>
                                                    <p>Attachment - <a style="color:blue"
                                                            href="/doctor/dashboard/report/<%= prevTreat.labReports.attachment %>">
                                                            <%= prevTreat.labReports.attachment %>
                                                        </a>
                                                    </p>
                                                    <p>Remarks - <%= prevTreat.labReports.remarks %>
                                                    </p>
                                                    <p>Dated - <%=
                                                            prevTreat.labReports.submitDate.toLocaleDateString("en-US")
                                                            %>
                                                    </p>
                                                    <% } else { %>
                                                        <p>No Reports</p>
                                                        <% } %>
                                            </div>
                                        </div>
                                        <div class="col-12 row">
                                            <div class="col-sm-4 bold">
                                                Response:
                                            </div>
                                            <div class="col-sm-8 text">
                                                <%= prevTreat.response %>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-dismiss"
                                            data-bs-dismiss="modal">Dismiss</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <% }) %>
                        <% } else {%>
                            <div class="no-item style=" text-align: center;"">
                                <p class="bold fs-20">No Previous Treatments</p>
                            </div>
                            <% } %>
        </div>
        <!-- For Lab Reports -->
        <div class="spaceFix"></div>
        <button type="button" class="collapsible fs-28">Lab Reports</button>
        <!-- All lab reports -->
        <div class="content-wrapper">
            <% if (patient.labReports.length> 0) { %>
                <!-- List of lab reports -->
                <% patient.labReports.forEach((report)=> { %>
                    <div class="wrapper mt-2">
                        <!-- block -->
                        <div class="content">
                            <div class="patient-detail">
                                <div class="name">
                                    <p class="bold">Treatment No :</p>
                                    <% if (report.treatmentNo===0) { %>
                                        <p>
                                            None
                                        </p>
                                        <% } %>
                                </div>
                                <div class="name">
                                    <p class="bold">Lab : </p>
                                    <p>
                                        <%= report.labName %>

                                    </p>
                                </div>
                                <div class="date">
                                    <p class="bold">Date:</p>
                                    <p>
                                        <%= report.submitDate.toLocaleDateString("en-GB") %>
                                    </p>
                                </div>
                            </div>
                            <div class="patient-action text-center">
                                <div class="view">
                                    <button class="btn btn-view" data-bs-toggle="modal"
                                        data-bs-target="#modal-<%= report._id %>">View</button>
                                </div>
                            </div>
                        </div>
                        <!-- modal -->
                        <div class="modal fade" id="modal-<%= report._id %>" tabindex="-1"
                            aria-labelledby="exampleModalLabel" aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title text-center" id="exampleModalLabel">Report Detail </h5>
                                        <button type="button" class="btn-close" data-bs-dismiss="modal"
                                            aria-label="Close"></button>
                                    </div>
                                    <div class="modal-body">

                                        <div class="col-12 row my-2">
                                            <div class="col-sm-4 bold">
                                                Lab :
                                            </div>
                                            <div class="col-sm-8 text">
                                                <%= report.labName %>
                                            </div>
                                        </div>
                                        <div class="col-12 row my-2">
                                            <div class="col-sm-4 bold">
                                                Treatment No :
                                            </div>
                                            <div class="col-sm-8 text">
                                                <% if (report.treatmentNo===0) { %>
                                                    <p>
                                                        None
                                                    </p>
                                                    <% } %>
                                            </div>
                                        </div>
                                        <div class="col-12 row my-2">
                                            <div class="col-sm-4 bold">
                                                Test :
                                            </div>
                                            <div class="col-sm-8 text">
                                                <%= report.testName %>
                                            </div>
                                        </div>
                                        <div class="col-12 row my-2">
                                            <div class="col-sm-4 bold">
                                                Attachment
                                            </div>
                                            <div class="col-sm-8 text">
                                                <%= report.attachment %>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-dismiss"
                                            data-bs-dismiss="modal">Dismiss</button>
                                        <a class="btn-download"
                                            href="/doctor/dashboard/report/<%= report.attachment %>">Download</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <% }) %>
                        <% } else { %>
                            <div class="no-item">
                                <p class="bold fs-20">No Sent Reports</p>
                            </div>
                            <% } %>
        </div>

        <!-- To Add new File -->
        <div class="addNew" data-toggle="tooltip" data-placement="top" title="Add new treatment">
            <form action="/doctor/dashboard/patient/form" method="POST">
                <input type="hidden" name="healthID" value="<%= patient.accessID.healthID %>">
